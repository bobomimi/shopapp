        <div class="container">

        <div class="row">
          <% @listings.each do |listing| %>
            <div class="col-md-3">
              <div class="thumbnail">
                <%= link_to image_tag(listing.image.url), listing, class: "img-responsive"%>
                <div class="caption">
                   <h3><%= listing.name %></h3>
                   <p><%= number_to_currency(listing.price) %></p>
                   <!--<p><%= t('sold_by', user: listing.user.name) %></p>-->
                </div>
              </div>
              
            </div>
          <% end %>   
        </div>
    </div>


